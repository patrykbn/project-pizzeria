{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "off"
        ],
        "no-console": [
            "off"
        ]
    },
    "globals": {
      "Handlebars": false
    }
    
}
